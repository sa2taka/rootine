{
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "import-name": false,
    "no-consecutive-blank-lines": false,
    "prettier": [
      true,
      {
        "printWidth": 180,
        "singleQuote": true,
        "trailingComma": "all",
        "semi": true
      }
    ]
  }
}
